{
	"keywords" : [
		"php",
		"formatter",
		"beautifier"
	],
	"autoload" : {
		"psr-4" : {
			"gossi\\formatter\\" : "src"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"gossi\\formatter\\tests\\" : "tests"
		}
	},
	"name" : "gossi/php-code-formatter",
	"type" : "library",
	"description" : "a code formatter for php code",
	"license" : "MIT",
	"require" : {
		"php" : ">=5.4",
		"symfony/config" : "~2",
		"symfony/yaml" : "~2",
		"gossi/collection" : "~1"
	},
	"require-dev" : {
		"phpunit/phpunit" : "~4"
	}
}